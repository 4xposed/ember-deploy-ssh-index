{
  "name": "ember-deploy-ssh-index",
  "version": "0.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "multi='dot=- html-cov=coverage.html' mocha -R mocha-multi --no-colors -u tdd ./node_tests/"
  },
  "config": {
    "blanket": {
      "pattern": "///[\\w-]+\\.js$/",
      "data-cover-never": [
        "node_modules",
        "public"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "repository": "https://github.com/treyhunner/ember-deploy-ssh-index/",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "blanket": "^1.1.6",
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-ember-hbs-template-compiler": "^1.6.1",
    "ember-cli": "0.1.15",
    "ember-cli-6to5": "^3.0.0",
    "ember-cli-app-version": "0.3.1",
    "ember-cli-content-security-policy": "0.3.0",
    "ember-cli-dependency-checker": "0.0.7",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.7",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.14.1",
    "ember-export-application-global": "^1.0.2",
    "express": "^4.8.5",
    "glob": "^4.0.5",
    "mocha": "^2.2.1",
    "proxyquire": "^1.4.0"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "dependencies": {
    "core-object": "^1.1.0",
    "ssh2": "^0.4.4"
  }
}
